{
  "name": "gulp-j140",
  "version": "0.0.2",
  "description": "Gulp plugin for j140 - Javascript template engine in 140 bytes, by Jed Schmidt. For browser and node.",
  "main": "index.js",
  "scripts": {
    "lint": "npm install && ./node_modules/jshint/bin/jshint ./lib/*.js ./test/*.js",
    "test": "npm run lint && ./node_modules/mocha/bin/mocha --reporter spec",
    "cov": "npm run lint && ./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha -- --reporter spec"
  },
  "author": {
    "name": "Charlike Mike Reagent",
    "email": "mameto_100@mail.bg",
    "url": "http://www.whistle-bg.tk",
    "twitter": "tunnckoCore"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tunnckoCore/gulp-j140.git"
  },
  "keywords": [
    "gulpplugin",
    "j140",
    "140bytes",
    "javascript",
    "template",
    "engine",
    "browser"
  ],
  "dependencies": {
    "gulp-util": "~2.2.17",
    "through2": "~0.5.1",
    "j140": "~0.0.2",
    "mocha-lcov-reporter": "0.0.1"
  },
  "devDependencies": {
    "coveralls": "*",
    "istanbul": "*",
    "jshint": "*",
    "mocha": "*",
    "gulp": "*"
  },
  "license": "MIT"
}
